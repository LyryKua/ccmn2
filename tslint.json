{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-react"
    ],
    "jsRules": {},
    "rules": {
      "quotemark": [
        true,
        "single",
        "jsx-double"
      ],
      "jsx-no-multiline-js": false,
      "ordered-imports": false,
      "no-console": {
        "severity": "warning",
        "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
      }
    },
    "rulesDirectory": []
}